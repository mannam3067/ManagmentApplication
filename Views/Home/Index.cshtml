@model IEnumerable<ManagmentApplication.Models.Employee>

@{
    //if we use _ViewStart.cshtml then no need to specify the layout name on view
    //Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Employee Information";
}

<html>
<body>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Department</th>
            </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Department</td>
            </tr>
        }

    </table>
</body>
</html>
